            if Default_Disable == false then
            Default_Disable = true;
            return;
        end
		if (hum) and hum.Health > 0 then
            --(function()
                hum.WalkSpeed = Value;
            --end)
        end
        end
    })
    
    local Slider = Tabs.Lplr:AddSlider("Slider", {
        Title = "Jump Power",
        Description = "Changer Your Jump Height (Power)",
        Default = 50,
        Min = 0,
        Max = 500,
        Rounding = 1,
        Callback = function(Value)
            if Default_Disable == false then
            Default_Disable = true;
            return;
        end
		if (hum) and hum.Health > 0 then
            pcall(function()
                hum.JumpPower = Value;
            end)
        end
        end
    })

